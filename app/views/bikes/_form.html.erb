<%= form_with(model: bike, local: true) do |form| %>
  
  <div class="field">
    <%= form.label :model %>
    <%= form.text_field :model, id: :bike_model %>
  </div>

  <div class="field">
    <%= form.label :brand %>
    <%= form.text_field :brand, id: :bike_brand %>
  </div>

   <div class="field">
    <%= form.hidden_field :user_id , :value => current_user.id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
